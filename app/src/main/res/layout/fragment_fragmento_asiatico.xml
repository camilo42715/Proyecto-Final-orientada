<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragmentoAsiatico">

    <!-- TODO: Update blank fragment layout -->

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/botonSuzuki"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="134dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="162dp"
            android:onClick="suzukyse"
            android:text="suzuky"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView" />

        <Button
            android:id="@+id/botonNissan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="134dp"
            android:layout_marginEnd="162dp"
            android:layout_marginBottom="17dp"
            android:onClick="nissanse"
            android:text="nissan"
            app:layout_constraintBottom_toTopOf="@+id/imageView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/botonToyota"
            style="@style/toyotase"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="134dp"
            android:layout_marginEnd="162dp"
            android:layout_marginBottom="16dp"
            android:text="toyota"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="152dp"
            android:layout_height="67dp"
            android:layout_marginStart="116dp"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="116dp"
            android:layout_marginBottom="15dp"
            android:src="@drawable/susuki"
            app:layout_constraintBottom_toTopOf="@+id/botonSuzuki"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="140dp"
            android:layout_height="76dp"
            android:layout_marginStart="116dp"
            android:layout_marginEnd="136dp"
            android:layout_marginBottom="14dp"
            android:src="@drawable/toyota"
            app:layout_constraintBottom_toTopOf="@+id/botonToyota"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="217dp"
            android:layout_height="90dp"
            android:layout_marginTop="14dp"
            android:layout_marginBottom="16dp"
            android:src="@drawable/nissan"
            app:layout_constraintBottom_toTopOf="@+id/botonNissan"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/botonSuzuki" />

    </android.support.constraint.ConstraintLayout>
</FrameLayout>